<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="galacticgames.android.skilltree">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".legacy.skill.SkillListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".legacy.log.LogListActivity">
        </activity>

        <activity
            android:name=".legacy.log.LogActivity"
            android:parentActivityName=".legacy.log.LogListActivity">
        </activity>

        <activity
            android:name=".legacy.skill.SkillActivity"
            android:parentActivityName=".legacy.skill.SkillListActivity">
        </activity>


    </application>

</manifest>